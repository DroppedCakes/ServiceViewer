<UserControl
    x:Class="MiniRisViewer.ServiceStatus.Views.ServiceStatus"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:MiniRisViewer.ServiceStatus.ViewModels"
    xmlns:local1="clr-namespace:MiniRisViewer.ServiceStatus.Helper"
    xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:prism="http://prismlibrary.com/"
    xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
    d:DesignHeight="768"
    d:DesignWidth="1024"
    prism:ViewModelLocator.AutoWireViewModel="True"
    mc:Ignorable="d">
    <UserControl.Resources>
        <local1:StatusConverter x:Key="StatusConverter" />
    </UserControl.Resources>
    <UserControl.DataContext>
        <local:ServiceStatusViewModel />
    </UserControl.DataContext>
    <!--<xctk:BusyIndicator BusyContent="{Binding ProgressMessage.Value}" IsBusy="{Binding InProgress.Value}">-->
    <!--    <Grid>-->
    <DockPanel>
        <GroupBox
            Margin="10"
            DockPanel.Dock="Top"
            Header="サービス稼働状況">

            <!--  ここからごにょごにょ  -->

            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="300" />
                    <ColumnDefinition Width="300" />
                    <ColumnDefinition Width="400" />
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="auto" />
                    <RowDefinition Height="auto" />
                    <RowDefinition Height="auto" />
                    <RowDefinition Height="auto" />
                    <RowDefinition Height="auto" />
                </Grid.RowDefinitions>
                <StackPanel
                    Grid.Row="0"
                    Grid.Column="0"
                    Orientation="Horizontal">
                    <TextBlock Style="{DynamicResource MaterialDesignTitleTextBlock}" Text="{Binding ImporterDisplayName, Mode=OneWay}" />
                </StackPanel>
                <StackPanel
                    Grid.Row="0"
                    Grid.Column="1"
                    Orientation="Horizontal">
                    <TextBlock Style="{DynamicResource MaterialDesignTitleTextBlock}" Text="{Binding ImporterStatus .Value, Converter={StaticResource StatusConverter}}" />
                </StackPanel>
                <StackPanel
                    x:Name="FileImporter"
                    Grid.Row="0"
                    Grid.Column="2"
                    VerticalAlignment="Top"
                    Orientation="Horizontal">
                    <Button
                        Width="100"
                        Margin="5"
                        Command="{Binding ImporterStartCommand}"
                        Content="Start" />
                    <Button
                        Width="100"
                        Margin="5"
                        Command="{Binding ImporterStopCommand}"
                        Content="Stop" />
                    <Button
                        Width="100"
                        Margin="5"
                        Command="{Binding ShowImporterLogCommand}"
                        Content="ShowLog" />
                </StackPanel>

                <StackPanel
                    Grid.Row="1"
                    Grid.Column="0"
                    Orientation="Horizontal">

                    <TextBlock Style="{DynamicResource MaterialDesignTitleTextBlock}" Text="{Binding ResponderDisplayName}" />
                </StackPanel>
                <StackPanel
                    Grid.Row="1"
                    Grid.Column="1"
                    Orientation="Horizontal">
                    <TextBlock Style="{DynamicResource MaterialDesignTitleTextBlock}" Text="{Binding ResponderStatus.Value　 , Converter={StaticResource StatusConverter}}" />
                </StackPanel>
                <StackPanel
                    Grid.Row="1"
                    Grid.Column="2"
                    VerticalAlignment="Top"
                    DockPanel.Dock="Top"
                    Orientation="Horizontal">
                    <Button
                        Width="100"
                        Margin="5"
                        Command="{Binding ResponderStartCommand}"
                        Content="Start" />
                    <Button
                        Width="100"
                        Margin="5"
                        Command="{Binding ResponderStopCommand}"
                        Content="Stop" />
                    <Button
                        Width="100"
                        Margin="5"
                        Command="{Binding ShowResponderLogCommand}"
                        Content="ShowLog" />
                </StackPanel>

                <StackPanel
                    Grid.Row="2"
                    Grid.Column="0"
                    Orientation="Horizontal">
                    <TextBlock Style="{DynamicResource MaterialDesignTitleTextBlock}" Text="{Binding ScpCoreDisplayName}" />
                </StackPanel>
                <StackPanel
                    Grid.Row="2"
                    Grid.Column="1"
                    Orientation="Horizontal">
                    <TextBlock Style="{DynamicResource MaterialDesignTitleTextBlock}" Text="{Binding ScpCoreStatus.Value, Converter={StaticResource StatusConverter}}" />
                </StackPanel>
                <StackPanel
                    Grid.Row="2"
                    Grid.Column="2"
                    VerticalAlignment="Top"
                    DockPanel.Dock="Top"
                    Orientation="Horizontal">
                    <Button
                        Width="100"
                        Margin="5"
                        Command="{Binding ScpCoreStartCommand}"
                        Content="Start" />
                    <Button
                        Width="100"
                        Margin="5"
                        Command="{Binding ScpCoreStopCommand}"
                        Content="Stop" />
                    <Button
                        Width="100"
                        Margin="5"
                        Command="{Binding ShowScpCoreLogCommand}"
                        Content="ShowLog" />
                </StackPanel>
                <StackPanel
                    Grid.Row="3"
                    Grid.Column="0"
                    Orientation="Horizontal">
                    <TextBlock Style="{DynamicResource MaterialDesignTitleTextBlock}" Text="{Binding AscDiplayName}" />
                </StackPanel>
                <StackPanel
                    Grid.Row="3"
                    Grid.Column="1"
                    Orientation="Horizontal">
                    <TextBlock Style="{DynamicResource MaterialDesignTitleTextBlock}" Text="{Binding AscStatus.Value, Converter={StaticResource StatusConverter}}" />
                </StackPanel>
                <StackPanel
                    Grid.Row="3"
                    Grid.Column="2"
                    VerticalAlignment="Top"
                    DockPanel.Dock="Top"
                    Orientation="Horizontal">
                    <Button
                        Width="100"
                        Margin="5"
                        Command="{Binding AscStartCommand}"
                        Content="Start" />
                    <Button
                        Width="100"
                        Margin="5"
                        Command="{Binding AscStopCommand}"
                        Content="Stop" />
                    <Button
                        Width="100"
                        Margin="5"
                        Command="{Binding ShowAscLogCommand}"
                        Content="ShowLog" />
                </StackPanel>

                <StackPanel
                    Grid.Row="4"
                    Grid.Column="0"
                    Orientation="Horizontal">
                    <TextBlock Style="{DynamicResource MaterialDesignTitleTextBlock}" Text="{Binding MppsDisplayName}" />
                </StackPanel>
                <StackPanel
                    Grid.Row="4"
                    Grid.Column="1"
                    Orientation="Horizontal">

                    <TextBlock Style="{DynamicResource MaterialDesignTitleTextBlock}" Text="{Binding MppsStatus.Value, Converter={StaticResource StatusConverter}}" />
                </StackPanel>
                <StackPanel
                    Grid.Row="4"
                    Grid.Column="2"
                    VerticalAlignment="Top"
                    DockPanel.Dock="Top"
                    Orientation="Horizontal">
                    <Button
                        Width="100"
                        Margin="5"
                        Command="{Binding MppsStartCommand}"
                        Content="Start" />
                    <Button
                        Width="100"
                        Margin="5"
                        Command="{Binding MppsStopCommand}"
                        Content="Stop" />
                    <Button
                        Width="100"
                        Margin="5"
                        Command="{Binding ShowMppsLogCommand}"
                        Content="ShowLog" />
                </StackPanel>

            </Grid>

